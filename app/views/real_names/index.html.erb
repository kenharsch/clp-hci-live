<h1>Listing real_names</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @real_names.each do |real_name| %>
      <tr>
        <td><%= link_to 'Show', real_name %></td>
        <td><%= link_to 'Edit', edit_real_name_path(real_name) %></td>
        <td><%= link_to 'Destroy', real_name, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Real name', new_real_name_path %>
