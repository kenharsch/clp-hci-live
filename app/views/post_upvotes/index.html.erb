<h1>Listing post_upvotes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @post_upvotes.each do |post_upvote| %>
      <tr>
        <td><%= link_to 'Show', post_upvote %></td>
        <td><%= link_to 'Edit', edit_post_upvote_path(post_upvote) %></td>
        <td><%= link_to 'Destroy', post_upvote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Post upvote', new_post_upvote_path %>
